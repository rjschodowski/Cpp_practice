/*
You are given a program that outputs all the numbers from 0 to 20.
Change the code to make it output only numbers that are multiples of 3.
*/

#include <iostream>
using namespace std;

int main()
{
    //change the code
    int num = 0;
    while(num<20){
        num+=1;
        if (num%3==0) {
            cout << num << endl;
        }
    }
    
    return 0;
}
